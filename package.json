{
  "name": "vite-plugin-cloudflare-workers-env-shims",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsdown --dts --unused --publint ./index.ts",
    "typecheck": "tsc --noEmit",
    "release": "bumpp",
    "prepack": "bun run build"
  },
  "exports": {
    ".": {
      "bun": "./index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "index.ts"
  ],
  "devDependencies": {
    "bumpp": "^10.1.0",
    "publint": "^0.3.12",
    "tsdown": "^0.9.6",
    "typescript": "^5",
    "unplugin-unused": "^0.4.4"
  },
  "peerDependencies": {
    "vite": "^6.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
